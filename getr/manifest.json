{
  "manifest_version": 2,
  "name": "Getr-Parcel",
  "version": "-1.0",

  "description": "Extension for a very specific website",
  "icons": {
  	"16": "icons/icon-16.svg"
  },
  "permissions": [
    "activeTab",
    "downloads",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icons/icon-16.svg",
    "default_title": "Get It!",
    "default_popup": "popup/menu.html"
  }
}